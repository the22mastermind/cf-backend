{
  "name": "cf-backend",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "start": "babel-node ./src/server.js",
    "dev-test": "cross-env NODE_ENV=test yarn pretest && nyc mocha --require @babel/register ./tests/*.js  --timeout 8000 --exit",
    "test": "cross-env NODE_ENV=test && nyc --reporter=lcov mocha --require @babel/register ./tests/*.js  --timeout 8000 --exit",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "dev": "cross-env NODE_ENV=development && nodemon --exec babel-node ./src/server.js",
    "lint": "eslint ./src",
    "db:create": "npx sequelize-cli db:create",
    "db:drop": "npx sequelize-cli db:drop",
    "db:migrate": "npx sequelize-cli db:migrate",
    "db:undo": "npx sequelize-cli db:migrate:undo:all",
    "pretest": "yarn db:drop && yarn db:create && yarn db:migrate && yarn seed",
    "seed": "npx sequelize-cli db:seed:all"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/the22mastermind/cf-backend.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/the22mastermind/cf-backend/issues"
  },
  "homepage": "https://github.com/the22mastermind/cf-backend#readme",
  "devDependencies": {
    "@babel/register": "^7.10.5",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "coveralls": "^3.1.0",
    "cross-env": "^7.0.2",
    "eslint": "^7.6.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-import": "^2.22.0",
    "mocha": "^8.1.1",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^2.0.4",
    "nyc": "^15.1.0"
  },
  "dependencies": {
    "@babel/core": "^7.11.1",
    "@babel/node": "^7.10.5",
    "@babel/plugin-syntax-class-properties": "^7.10.4",
    "@babel/plugin-transform-runtime": "^7.11.0",
    "@babel/preset-env": "^7.11.0",
    "babel-eslint": "^10.1.0",
    "babel-plugin-istanbul": "^6.0.0",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "joi": "^17.2.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.19",
    "mailgen": "^2.0.13",
    "make-runnable": "^1.3.6",
    "moment": "^2.27.0",
    "morgan": "^1.10.0",
    "pg": "^8.3.0",
    "pg-hstore": "^2.3.3",
    "sequelize": "^5.21.6",
    "sequelize-cli": "^5.5.1",
    "sequelize-test-helpers": "^1.2.2",
    "sinon": "^9.0.3",
    "sinon-chai": "^3.5.0",
    "twilio": "^3.48.2"
  }
}
